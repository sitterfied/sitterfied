<!-- /top note -->
<div class="top_info clear">
  <div class="find_form_block">
	<p>Where and when do you need your sitter?</p>
	<form>
	  <ul class="clear">
		<li>
		  <label for="where">Where</label>
          {{view Ember.TextField valueBinding="zip" placeholder="ZIPCODE"}}
		</li>
		<li>
		  <label for="when">{{# unless overnight }}When{{else}}From{{/unless}}</label>
            {{view Ember.DatePicker placeholder="DATE" id="datepicker" class="datepicker" valueBinding="start_date"}}
		</li>
		<li class="fatselect">
		  <label for="from">From</label>
          {{view Sitterfied.Select2
          contentBinding="Sitterfied.HourBlocks"
          optionLabelPath="content.name"
          optionValuePath="content.value"
          valueBinding="start_time"
          id="from" prompt=""
          }}
		</li>
		<li class="fatselect">
		  <label for="to">To</label>
          {{view Sitterfied.Select2
          contentBinding="Sitterfied.HourBlocks"
          optionLabelPath="content.name"
          optionValuePath="content.value"
          valueBinding="stop_time"
          id="to" prompt=""
          }}
		</li>
		<li class="fatselect">
		  <label for="kids">For</label>
          {{view Sitterfied.Select2
          contentBinding="Sitterfied.Kids"
          optionLabelPath="content.name"
          optionValuePath="content.value"
          valueBinding="kids" prompt=""
          id="for"
          }}
		</li>
		<li>
          <label>&nbsp;</label>
          {{view Sitterfied.AjaxStateButton valueBinding="isLoading" action="findSitters" class="findSitter" saveText="FIND"}}
		</li>
	  </ul>
	</form>
  </div>
</div>
