{{# linkTo search class="back"}}Back to search results{{/linkTo}}
<!-- top info -->
<div class="top_info clear">
  <div class="member_since guest"></div>
  <div class="photo">
	<div class="photo_edit_wrap">
      <img {{bindAttr src=avatarUrl alt=full_name}} />
	</div>
  </div>
  <div class="guest_top_info">
	<p>{{full_name}}</p>
	<p>{{children.length}} Children &nbsp;|&nbsp; {{city}}, {{state}}</p>
	<div class="know">
	  <p>Do you know {{first_name}}?</p>
	  <p>Connect with this person via Facebook or Gmail to have them automatically added to your Sitterfied network. <a href="#">Learn more</a>.</p>
	</div>
  </div>
</div>
<!-- /top info -->
<!-- quick_info_list -->
{{# view Sitterfied.FixedView class="quick_info_list clear" id="fixed_tabs" }}
  <ul>
	<li><strong>{{friends.length}}</strong> friends</li>
	<li><strong>{{sitter_groups.length}}</strong>groups</li>
  </ul>
{{/ view}}
<!-- /quick_info_list -->