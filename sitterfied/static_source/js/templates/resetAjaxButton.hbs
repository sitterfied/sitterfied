<a class="button button_small button_cancel" {{action "doAction" target=view}}>Reset</a>
{{# if view.isLoading }}
  <img class="loading" src="/static/images/loading.gif">
{{/ if}}
{{# if view.isSaved }}
  <span class="saved">Saved!</span>
{{/ if}}
{{# if view.isError }}
  <span class="error">There was an error saving your data</span>
{{/ if}}