            {{/ each}}
		  </ul>
		</div>
		<!-- /upcomingJobs_block -->

		<div class="mob_tab_trigger"><span>{{completedJobs.length}}</span>Completed Jobs</div>
		<!-- completed_jobs_block -->
		<div class="subtab_content completed_jobs_block" id="tab-1-3">
          <ul class="booking_list">
		  {{# each booking in completedJobs }}
