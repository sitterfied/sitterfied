<!-- bookings_block -->
	  <div class="mob_tab_trigger">Bookings</div>
	  <div class="tab_content bookings_block" id="tab-3">
		<ul class="subtab_select">
		  <li><a {{action zoomToPending}}><span>{{pendingRequests.length}}</span>Pending Requests</a></li>
		  <li><a {{action zoomToUpcoming}}><span>{{upcomingJobs.length}}</span>Upcoming Jobs</a></li>
		  <li><a {{action zoomToCompleted}}"><span>{{completedJobs.length}}</span>Completed Jobs</a></li>
		  <li><a {{action zoomToMissed}}><span>{{missedRequests.length}}</span>Missed Requests</a></li>
		  <li><a {{action zoomToDeclined}}><span>{{declinedRequests.length}}</span>Declined Jobs</a></li>
		  <li><a {{action zoomToCanceled }}><span>{{canceledRequests.length}}</span>Canceled Jobs</a></li>
		</ul>
		<div class="mob_tab_trigger active"><span>{{pendingRequests.length}}</span>Pending Requests</div>
		<!-- pending_requests_block -->
		<div class="subtab_content pending_requests_block" id="tab-1-1">
		  <ul class="booking_list">
            {{# each booking in pendingRequests }}
              {{ render "booking" booking }}
            {{/ each}}
		  </ul>
		</div>
		<!-- /pending_jobs_block -->

		<div class="mob_tab_trigger "><span>{{upcomingJobs.length}}</span>Upcoming Jobs</div>
